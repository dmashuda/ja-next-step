<%= simple_form_for([@stakeholder, @profile], :url => url_for(:action => 'update'), :html => {:multipart => true}) do |f| %>

    <%= f.input :website_url %>
    <%= f.input :facebook_url %>
    <%= f.input :youtube_url %>
    <%= f.input :twitter_url %>
    <%= f.input :instagram_url %>
    <%= f.input :name %>
    <%= f.input :email, as: :email %>
    <%= f.input :phone %>
    <%= f.input :street %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip %>
    <%= f.label :logo %>
    <%= f.file_field :logo %>
    <%= f.file_field :banner %>

    <%= f.button :submit %>

<% end %>
