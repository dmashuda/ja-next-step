<%= simple_form_for(@post, html: {multipart: true}) do |f| %>

    <%= f.input :stakeholder_id, as: :hidden %>
    <%= f.input :subject %>
    <%= f.input :start_date, as: :string, input_html: {type: :date} %>
    <%= f.input :end_date, as: :string, input_html: {type: :date} %>
    <%= f.input :abstract %>
    <%= f.input :body %>
    <%= f.input :contact_email %>
    <%= f.input :email_subject %>
    <%= f.input :email_body %>
    <%= f.input :active %>
    <%= f.file_field :photo %>

    <%= f.association :keywords, label_method: :label, as: :check_boxes %>

    <%= f.button :submit %>
<% end %>


